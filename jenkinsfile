pipeline {
   tools {
      nodejs 'node'
    }
    stages{
        stage('Clean workspace') {
             steps {
                echo 'Cleaning Workspace...'
                deleteDir()
            }
        stage('checkout') {
            steps {
                git url: 'https://github.com/gayathri-panneerselvam/frontendapp.git'
            }
        }
        stage('build'){
            steps {
                sh 'npm install'
            }
        }

    }
}
